{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.16",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "axios": "^1.3.4",
        "flowbite": "^1.6.4",
        "flowbite-react": "^0.4.2",
        "framer-motion": "^10.5.0",
        "react": "^18.2.0",
        "react-cookie": "^4.1.1",
        "react-dom": "^18.2.0",
        "react-icons": "^4.8.0",
        "react-intersection-observer": "^9.4.3",
        "react-query": "^3.39.3",
        "react-router-dom": "^6.9.0",
        "react-scripts": "5.0.1",
        "react-toastify": "^9.1.1",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build && npm run collect",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "collect": "npm run build-js && npm run build-css && npm run build-copy-html",
        "build-js": "npm run build-copy-js && npm run build-rename-js && npm run build-rename-js-map ",
        "build-css": "npm run build-copy-css && npm run build-rename-css && npm run build-rename-css-map",
        "build-copy-js": "rm -rf ./static/js/* && cp -r ./build/static/js/* ./static/js/",
        "build-copy-css": "rm -rf ./static/css/* && cp -r ./build/static/css/* ./static/css/",
        "build-rename-js": "renamer --find '/^main.*js$/i' --replace frontend-ui.js static/js/*.js",
        "build-rename-js-map": "renamer --find '/^main.*js.map$/i' --replace frontend-ui.js.map static/js/*.js.map",
        "build-rename-css": "renamer --find '/^main.*css$/i' --replace frontend-ui.css static/css/*.css",
        "build-rename-css-map": "renamer --find '/^main.*css.map$/i' --replace frontend-ui.css.map static/css/*.css.map",
        "build-copy-html": "rm -rf ./templates/index.html && cp -r ./build/index.html ./templates/index.html"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "autoprefixer": "^10.4.14",
        "postcss": "^8.4.21",
        "prettier": "^2.8.4",
        "prettier-plugin-tailwindcss": "^0.2.4",
        "renamer": "^4.0.0",
        "tailwind-scrollbar-hide": "^1.1.7",
        "tailwindcss": "^3.2.7"
    }
}
